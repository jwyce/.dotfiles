{
	"[cpp]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"[elixir]": {
		"editor.defaultFormatter": "saratravi.elixir-formatter"
	},
	"[go]": {
		"editor.defaultFormatter": "golang.go"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[markdown]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"diffEditor.ignoreTrimWhitespace": false,
	"discord.idleTimeout": 1200,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": true,
	"editor.cursorSurroundingLines": 8,
	"editor.detectIndentation": false,
	"editor.fontFamily": "Dank Mono, Monaco, 'Apple Color Emoji', 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 14,
	"editor.formatOnSave": true,
	"editor.inlineSuggest.enabled": true,
	"editor.lineNumbers": "relative",
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.tabSize": 2,
	"files.exclude": {
		"**/*.meta": true
	},
	"git.autofetch": true,
	"git.confirmSync": false,
	"github.copilot.enable": {
		"*": true,
		"markdown": true,
		"plaintext": false,
		"yaml": false
	},
	"howIVSCode.userIdentificationKey": " 6a88a47235b48b0af0c3d768b9bdfc",
	"importCost.debug": false,
	"importSorter.importStringConfiguration.tabSize": 2,
	"javascript.format.semicolons": "insert",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"jest.autoRun": {
		//"onStartup": ["all-tests"]
	},
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"python.languageServer": "Default",
	"react-native-tools.showUserTips": false,
	"redhat.telemetry.enabled": true,
	"rust-analyzer.cargo.features": "all",
	"terminal.external.osxExec": "iterm2.app",
	"terminal.integrated.defaultProfile.osx": "fish",
	"terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
	"terminal.integrated.fontFamily": "MesloLGS NF, Monaco, 'Apple Color Emoji', 'Courier New', monospace",
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.gpuAcceleration": "canvas",
	"terminal.integrated.profiles.linux": {
		"fish": {
			"icon": "terminal-ubuntu",
			"path": "/usr/bin/fish"
		}
	},
	"terminal.integrated.profiles.osx": {
		"fish": {
			"icon": "terminal-tmux",
			"id": "zsh",
			"path": "/bin/zsh"
		}
	},
	"terminal.integrated.profiles.windows": {
		"PowerShell": {
			"icon": "terminal-powershell",
			"source": "PowerShell"
		}
	},
	"todo-tree.filtering.excludeGlobs": ["**/node_modules", "**.d.ts"],
	"todo-tree.general.tagGroups": {
		"FIXME": ["FIXME", "FIXIT", "FIX"]
	},
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"REMARK",
		"NOTE",
		"[ ]",
		"[x]"
	],
	"todo-tree.highlights.customHighlight": {
		"BUG": {
			"background": "#aadd11",
			"foreground": "#111",
			"gutterIcon": true,
			"iconColour": "#aadd11"
		},
		"FIXME": {
			"background": "#Ff2047",
			"foreground": "#fff",
			"gutterIcon": true,
			"iconColour": "#Ff2047"
		},
		"HACK": {
			"background": "#8A2BE2",
			"foreground": "#fff",
			"gutterIcon": true,
			"iconColour": "#8A2BE2"
		},
		"NOTE": {
			"background": "#FFD700",
			"foreground": "#111",
			"gutterIcon": true,
			"iconColour": "#FFD700"
		},
		"REMARK": {
			"background": "#aaa",
			"foreground": "#111",
			"gutterIcon": true,
			"iconColour": "#aaa"
		},
		"TODO": {
			"background": "#17b9ff",
			"foreground": "#fff",
			"gutterIcon": true,
			"iconColour": "#17b9ff"
		}
	},
	"todo-tree.highlights.useColourScheme": true,
	"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
	"typescript.format.semicolons": "insert",
	"typescript.tsserver.experimental.enableProjectDiagnostics": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"vim.insertModeKeyBindings": [
		{
			"after": ["<esc>"],
			"before": ["j", "k"]
		}
	],
	"vim.leader": " ",
	"vim.normalModeKeyBindings": [
		{
			"after": ["\"", "+", "y"],
			"before": ["<leader>", "y"]
		},
		{
			"before": ["g", "d"],
			"commands": ["editor.action.revealDefinition"]
		},
		{
			"before": ["<leader>", "v", "r", "r"],
			"commands": ["editor.action.goToReferences"]
		},
		{
			"before": ["<leader>", "s"],
			"commands": ["editor.action.rename"]
		},
		{
			"before": ["<leader>", "p", "f"],
			"commands": ["workbench.action.quickOpen"]
		},
		{
			"before": ["<leader>", "p", "c"],
			"commands": ["workbench.action.showCommands"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.showHover"]
		},
		{
			"before": ["<C-o>"],
			"commands": ["workbench.action.closeActiveEditor"]
		},
		{
			"before": ["<leader>", "a"],
			"commands": ["vscode-harpoon.addEditor"]
		},
		{
			"before": ["<leader>", "e"],
			"commands": ["vscode-harpoon.editEditors"]
		},
		{
			"before": ["<leader>", "p", "e"],
			"commands": ["vscode-harpoon.editorQuickPick"]
		},
		{
			"before": ["<leader>", "J"],
			"commands": ["vscode-harpoon.gotoEditor1"]
		},
		{
			"before": ["<leader>", "K"],
			"commands": ["vscode-harpoon.gotoEditor2"]
		},
		{
			"before": ["<leader>", "L"],
			"commands": ["vscode-harpoon.gotoEditor3"]
		},
		{
			"before": ["<leader>", ":"],
			"commands": ["vscode-harpoon.gotoEditor4"]
		}
	],
	"vim.useSystemClipboard": false,
	"vim.visualModeKeyBindings": [
		{
			"after": ["\"", "+", "y"],
			"before": ["<leader>", "y"]
		},
		{
			"after": ["\"", "_", "d", "P"],
			"before": ["<leader>", "p"]
		},
		{
			"before": ["J"],
			"commands": ["editor.action.moveLinesDownAction"]
		},
		{
			"before": ["K"],
			"commands": ["editor.action.moveLinesUpAction"]
		}
	],
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"workbench.colorCustomizations": {
		"activityBar.foreground": "#d297f5",
		"activityBarBadge.background": "#a95ffd",
		"badge.background": "#a95ffd",
		"border": "#d297f5",
		"editorBracketHighlight.foreground1": "#ffaab7",
		"editorBracketHighlight.foreground2": "#9b97f9",
		"editorBracketHighlight.foreground3": "#bfff88",
		"editorBracketHighlight.foreground4": "#81ecd5",
		"editorBracketHighlight.foreground5": "#a44a8c",
		"editorInlayHint.background": "#723500",
		"editorInlayHint.foreground": "#9a9a9a",
		"editorLineNumber.activeForeground": "#d297f5",
		"editorLineNumber.foreground": "#9268aa",
		"panel.border": "#a878c4",
		"panel.dropBorder": "#a878c4",
		"panelTitle.activeBorder": "#d297f5",
		"tab.activeBorderTop": "#d297f5",
		"terminal.tab.activeBorder": "#d297f5"
	},
	"workbench.colorTheme": "Bearded Theme Arc Reversed",
	"workbench.editor.enablePreview": false,
	"workbench.editor.wrapTabs": true,
	"workbench.iconTheme": "bearded-icons",
	"workbench.productIconTheme": "fluent-icons",
  "workbench.sideBar.location": "right"
}
